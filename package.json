{
  "name": "@uxland/uxl-rich-text-editor",
  "version": "0.0.0-development",
  "description": "uxl-rich-text-editor",
  "main": "uxl-rich-text-editor.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxland/uxl-rich-text-editor.git"
  },
  "keywords": [
    "web-components",
    "web-component",
    "polymer"
  ],
  "author": "UXLand",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/uxland/uxl-rich-text-editor/issues"
  },
  "homepage": "https://github.com/uxland/uxl-rich-text-editor",
  "files": [
    "uxl-rich-text-editor.js",
    "*.js",
    "src/*/*/.js",
    "vendor",
    "theme"
  ],
  "scripts": {
    "deps:delete": "rimraf node_modules/ package-lock.json",
    "deps:install": "npm install",
    "deps:refresh": "npm run deps:delete && npm run deps:install",
    "build": "tsc -p tsconfig.json || echo 'Build completed'",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "enable-sass-watcher": "sass-render ./src/**/*.scss -w --suffix .js",
    "compile-sass": "sass-render ./src/**/*.scss --suffix .ts",
    "compile-ts": "tsc -p tsconfig.json",
    "polymer:serve": "polymer serve"
  },
  "dependencies": {
    "@polymer/lit-element": "^0.6.5",
    "@uxland/uxl-polymer2-ts": "^1.0.11",
    "@uxland/uxl-prism": "^1.0.8",
    "quill": "^1.3.6",
    "tslib": "^1.9.3",
    "wc-sass-render": "^1.1.1"
  },
  "devDependencies": {
    "@webcomponents/custom-elements": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.2.0",
    "dts-generator": "^2.1.0",
    "gulp": "^4.0.0",
    "gulp-run": "^1.7.1",
    "mocha": "^5.2.0",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.6"
  }
}
